{
    "typescript.validate.enable": true,
    "javascript.validate.enable": true,
    "javascript.suggestionActions.enabled": false,
    "eslint.enable": true,
    "eslint.validate": [
        "vue",
        "html",
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact"
    ],
    "eslint.workingDirectories": [
        "./src"
    ],
    "typescript.preferences.importModuleSpecifier": "relative",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/dist": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
    }
}